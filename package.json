{
  "name": "omgind-react",
  "version": "3.0.0",
  "description": "RBAC权限管理脚手架",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "lint:style": "stylelint \"src/**/*.less\" --custom-syntax postcss-less",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src && npm run lint:style",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx src && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx src"
  },
  "dependencies": {
    "@ant-design/compatible": "^5.1.1",
    "@ant-design/icons": "4.8.0",
    "@ant-design/pro-components": "^2.3.57",
    "@ant-design/pro-descriptions": "^2.0.49",
    "@ant-design/pro-layout": "^7.8.2",
    "antd": "^5.3.0",
    "axios": "^0.21.1",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "dva": "^2.4.1",
    "form-render": "^2.0.10",
    "history": "^4.7.2",
    "lodash": "^4.17.21",
    "lodash-decorators": "^6.0.0",
    "md5": "^2.2.1",
    "path-to-regexp": "^2.4.0",
    "performant-array-to-tree": "^1.11.0",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-container-query": "^0.11.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^18.2.0",
    "setprototypeof": "^1.1.0",
    "umi": "^3.5.36",
    "url-polyfill": "^1.0.10",
    "uuid": "^3.3.2",
    "yarn-check": "^0.0.3"
  },
  "devDependencies": {
    "@ant-design/antd-theme-variable": "^1.0.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-decorators": "^7.20.13",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@types/react": "^16.7.7",
    "@types/react-dom": "^16.0.10",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@umijs/preset-react": "^1",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.4.0",
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-compat": "^4.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^1.2.0",
    "less-loader": "^7.0.0",
    "lint-staged": "^13.1.2",
    "postcss": "8.4.21",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.4",
    "stylelint": "^15.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-less": "^1.0.4",
    "stylelint-config-standard": "^30.0.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,less}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.less": "stylelint --custom-syntax postcss-less"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 12",
    "not dead"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  }
}
